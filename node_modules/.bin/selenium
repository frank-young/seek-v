#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../.npminstall/selenium-server/2.53.1/selenium-server/bin/selenium" "$@"
  ret=$?
else 
  node  "$basedir/../.npminstall/selenium-server/2.53.1/selenium-server/bin/selenium" "$@"
  ret=$?
fi
exit $ret
