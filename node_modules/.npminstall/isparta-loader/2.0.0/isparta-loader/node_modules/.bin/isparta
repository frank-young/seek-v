#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../../isparta/4.0.0/isparta/bin/isparta" "$@"
  ret=$?
else 
  node  "$basedir/../../../../../isparta/4.0.0/isparta/bin/isparta" "$@"
  ret=$?
fi
exit $ret
